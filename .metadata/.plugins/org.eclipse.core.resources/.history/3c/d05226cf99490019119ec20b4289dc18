package Controller;

public class Error {
	private int errorCode; 
	private String message; 
	
	public Error (int errorCode, String message) { 
		this.errorCode = errorCode; 
		this.message = message; 
	}
	
	public int getErrorCode() { 
		return this.errorCode; 
	}
	
	public String getMessage() { 
		return this.message;
	}
}
